<!DOCTYPE html>
<html lang="en">
    <head>       
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <meta name="author" content="">
        <title>{% block title %}Welcome Misha!{% endblock %}</title>
        {% block stylesheets %}
            {#<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
           
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/css/dashboard.css') }}" />#}
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/css/sb-admin-2.css') }}" />
            <link rel="stylesheet" href="https://cdn.datatables.net/1.10.4/css/jquery.dataTables.css">
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/css/plugins/metisMenu/metisMenu.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/css/plugins/timeline.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/css/plugins/morris.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/acmestore/font-awesome/css/font-awesome.min.css') }}" />
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% endblock %}      
        <!-- jQuery -->
        <script src="{{ asset('bundles/acmestore/js/jquery.js') }}"></script>
    </head>
    <body>


        <div id="wrapper">

            <!-- Navigation -->
            <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="index.html">SB Admin v2.0</a>
                </div>
                <!-- /.navbar-header -->
                {#{{ knp_menu_render('AcmeStoreBundle:Builder:mainMenu', {'currentClass': 'active'}) }} #}
                <ul class="nav navbar-top-links navbar-right">

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-user fa-fw"></i>  <i class="fa fa-caret-down"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li><a href="#"><i class="fa fa-user fa-fw"></i> User Profile</a>
                            </li>
                            <li><a href="#"><i class="fa fa-gear fa-fw"></i> Settings</a>
                            </li>
                            <li class="divider"></li>
                            <li><a href="{{ path('fos_user_security_logout') }}"><i class="fa fa-sign-out fa-fw"></i> Logout</a>
                            </li>
                        </ul>
                        <!-- /.dropdown-user -->
                    </li>
                    <!-- /.dropdown -->
                </ul>
                <!-- /.navbar-top-links -->

                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        {{ knp_menu_render('AcmeStoreBundle:Builder:sidebarMenu', {'currentClass': 'active', 'template':'AcmeStoreBundle:Menu:knp_menu.html.twig'}) }}                                        
                    </div>
                    <!-- /.sidebar-collapse -->
                </div>
                <!-- /.navbar-static-side -->
            </nav>

            <div id="page-wrapper">
                <br>
                <div class="col-sm-8">
                    {% for flashMessage in app.session.flashbag.get('success') %}
                        <div class="alert alert-success alert-dismissible" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <strong>Succes!</strong> {{ flashMessage }}
                        </div>
                    {% endfor %}
                    {% for flashMessage in app.session.flashbag.get('notice') %}
                        <div class="alert alert-warning alert-dismissible" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <strong>Warning!</strong> {{ flashMessage }}
                        </div>
                    {% endfor %}
                    {% for flashMessage in app.session.flashbag.get('danger') %}
                        <div class="alert alert-danger alert-dismissible" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <strong>Danger!</strong> {{ flashMessage }}
                        </div>
                    {% endfor %}
                </div>
                {% block body %}




                {% endblock %}
            </div>
            <!-- /#page-wrapper -->
        </div>
        <!-- /#wrapper -->

        {% block javascripts %}
            {# <script src="https://code.jquery.com/jquery-1.11.2.min.js"></script>
             <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
              #}



            <!-- Bootstrap Core JavaScript -->
            <script src="{{ asset('bundles/acmestore/js/bootstrap.min.js') }}"></script>

            <script src="https://cdn.datatables.net/1.10.4/js/jquery.dataTables.min.js"></script>

            <!-- Metis Menu Plugin JavaScript -->
            <script src="{{ asset('bundles/acmestore/js/plugins/metisMenu/metisMenu.min.js') }}"></script>

            <!-- Morris Charts JavaScript -->
           <!-- <script src="{{ asset('bundles/acmestore/js/plugins/morris/raphael.min.js') }}"></script>
            <script src="{{ asset('bundles/acmestore/js/plugins/morris/morris.min.js') }}"></script>
            <script src="{{ asset('bundles/acmestore/js/plugins/morris/morris-data.js') }}"></script>-->

            <!-- Custom Theme JavaScript -->
            <script src="{{ asset('bundles/acmestore/js/sb-admin-2.js') }}"></script>
            {# <script src="{{ asset('bundles/acmestore/js/jquery.confirm.js') }}"></script>#}
            <script src="{{ asset('bundles/acmestore/js/ready.js') }}"></script> 
        {% endblock %}
    </body>
</html>
